{
  "extends": "astro/tsconfigs/strict",
  "exclude": [
    "node_modules",
    "public",
    "dist",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
